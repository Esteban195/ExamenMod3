pipeline {
    agent any
    stages{
    stage('Newman Tests') {
        steps {
            sh '''
            cd api/newman
            newman run ExamenModulo3.json -e ExamenMod3.json
            '''
        }
    }
    stage('JMeter Tests') {
        steps {
            sh '''
            ls
            export PATH=/home/pjcalvov/jmeter/apache-jmeter-5.3/bin:$PATH
            cd load
            jmeter -n -t ExamenU.jmx
            '''
        }
    }
    }
}